import sys
input = sys.stdin.readline
n, m = map(int, input().split())
real = input().split()
arr = []
for i in range(m):
  a, b, c = map(int, input().split())
  arr.append([b, c])

s = []
visited = [False] * (n + 1)
def backtracking(start):
    if len(s) == m:
        print(' '.join(map(str, s)))
        return
    
    for i in range(m):
        if not visited[i] :
          visited[i] = True
          
            s.append(i)
            backtracking(i + 1)
            s.pop()
backtracking(1)
 